<urlrewritingnet rewriteOnlyVirtualUrls="true" contextItemsPrefix="QueryString" defaultPage="index.htm" defaultProvider="RegEx" xmlns="http://www.urlrewriting.net/schemas/config/2006/07">
    <rewrites>
        <add name="SocialShopPortal" virtualUrl="^~/index.htm" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/Default.aspx" ignoreCase="true"/>
        <add name="Login" virtualUrl="^~/login" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Login.aspx" ignoreCase="true"/>
        <add name="NewsDetails" virtualUrl="^~/news/(.*)-(.*)/(.*)-(.*)" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/NewsDetails.aspx?CategoryID=$2&amp;NewsID=$4" ignoreCase="true"/>
        <add name="News" virtualUrl="^~/news/(.*)-(.*)" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/News.aspx?CategoryID=$2" ignoreCase="true"/>
        <add name="Document" virtualUrl="^~/document/(.*)-(.*)" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/Document.aspx?CategoryID=$2" ignoreCase="true"/>
        <add name="realtymarket" virtualUrl="^~/realtymarket/(.*)-(.*)" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/realtymarket.aspx?CategoryID=$2" ignoreCase="true"/>
        <add name="AddRealtymarket" virtualUrl="^~/addrealtymarket" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/BdsAddNews.aspx" ignoreCase="true"/>
        <add name="Contact" virtualUrl="^~/contact" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/pages/Contact.aspx" ignoreCase="true"/>

        

        <add name="CategoryManager" virtualUrl="^~/admin/category" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/Category.aspx" ignoreCase="true"/>
        <add name="NewsManager" virtualUrl="^~/admin/news" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/News.aspx" ignoreCase="true"/>
        <add name="AdminLogin" virtualUrl="^~/admin/login" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/Login.aspx" ignoreCase="true"/>
        <add name="AdminNotPermiss" virtualUrl="^~/admin/notpermission" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/NotPermiss.aspx" ignoreCase="true"/>

        <add name="AdminFunction" virtualUrl="^~/admin/function" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/account/Function.aspx" ignoreCase="true"/>
        <add name="AdminRole" virtualUrl="^~/admin/role" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/account/Role.aspx" ignoreCase="true"/>
        <add name="AdminAdmin" virtualUrl="^~/admin/admin" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/account/Admin.aspx" ignoreCase="true"/>
        <add name="AdminLog" virtualUrl="^~/admin/log" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/account/Log.aspx" ignoreCase="true"/>

        <add name="AdminPartners" virtualUrl="^~/admin/partners" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/Partners.aspx" ignoreCase="true"/>
        <add name="AdminSlideShow" virtualUrl="^~/admin/slideshow" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/SlideShow.aspx" ignoreCase="true"/>
        <add name="AdminDocument" virtualUrl="^~/admin/document" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/Document.aspx" ignoreCase="true"/>
        <add name="AdminEmailContact" virtualUrl="^~/admin/emailcontact" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/EmailContact.aspx" ignoreCase="true"/>
        <add name="AdminRealtyMarket" virtualUrl="^~/admin/market" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/pages/content/RealtyMarket.aspx" ignoreCase="true"/>
        
        <add name="HomeBackend" virtualUrl="^~/admin" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/Default.aspx" ignoreCase="true"/>
        <add name="HomeBackend1" virtualUrl="^~/admin/index.htm" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Backend/Default.aspx" ignoreCase="true"/>
    </rewrites>
</urlrewritingnet>